<div class="page-header">
  <%= link_to warehouses_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    <%= t('views.back') %>
  <% end %>
  <%= link_to edit_warehouse_path(@warehouse), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    <%= t('views.edit') %>
  <% end %>
  <h1><%= t 'views.show_model', :model => t('activerecord.models.warehouse') %></h1>
</div>

<dl class="dl-horizontal">
  <dt><%= t('activerecord.attributes.warehouse.name') %>:</dt>
  <dd><%= @warehouse.name %></dd>

  <dt><%= t('activerecord.attributes.warehouse.description') %>:</dt>
  <dd><%= @warehouse.description %></dd>

</dl>
